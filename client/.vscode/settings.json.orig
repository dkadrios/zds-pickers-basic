  ],
  "files.exclude": {
    "**/*.flow": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    ".cache": true,
    ".vscode": true,
    "coverage": true,
    "dist": true,
    "flow-coverage": true,
    "node_modules": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "html.format.endWithNewline": true,
  "javascript.validate.enable": false,
  "search.exclude": {
    "**/bower_components": true,
    "**/node_modules": true,
    "build": true,
    "coverage": true,
    "dist": true,
