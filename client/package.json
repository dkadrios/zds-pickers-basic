{
  "name": "zds-shifter-pro-client",
  "version": "1.0.0",
  "description": "Web client for ZDS Shifter Pro",
  "license": "UNLICENSED",
  "scripts": {
    "build:prod": "NODE_ENV=production parcel build src/index.html --no-source-maps --detailed-report --no-cache --no-autoinstall",
    "build:report": "open ./dist/report.html",
    "build:shake": "NODE_ENV=production parcel build src/index.html --no-source-maps --detailed-report --no-cache --no-autoinstall --experimental-scope-hoisting",
    "build": "yarn run clean && yarn run build:prod",
    "clean": "rimraf dist",
    "cov": "open ./coverage/lcov-report/index.html",
    "dev": "parcel src/index.html --port 9000 --no-autoinstall",
    "devs": "parcel src/index.html --port 9000 --no-autoinstall --https --cert ~/.sstls/cert.pem --key ~/.sstls/privkey.pem",
    "docs": "docker-compose up --build docs",
    "e2e": "jest --coverage --config=__e2e__/jest.config.js",
    "e2e:ci": "HEADLESS=true yarn run e2e",
    "eslint-check": "eslint --print-config path/to/main.js | eslint-config-prettier-check",
    "graph": "madge --exclude spec.js --image .module-graph.svg src",
    "lint": "eslint . --ext js,json,md,mdx",
    "lint:fix": "yarn run lint --fix",
    "reset": "rm -rf node_modules && rm -rf .cache && yarn",
    "sb": "start-storybook --port 9001 --ci",
    "sb:ci": "NODE_ENV=staging build-storybook -c .storybook -o .storybook/site",
    "shake": "yarn run clean && yarn run build:shake",
    "simulate": "yarn build && docker-compose up --build simulate",
    "unit": "jest --coverage",
    "unit:ci": "node --expose-gc ./node_modules/.bin/jest --config=jest-ci.config.js --logHeapUsage",
    "unit:gc": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage",
    "unit:wc": "yarn unit --watch --changedSince=main --passWithNoTests --coverageThreshold={}"
  },
  "author": "Darin Kadrioski <darin@nebiru.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:dkadrios/zds-shifter-pro.git"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-export-default-from": "7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "7.12.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/plugin-transform-runtime": "7.12.10",
